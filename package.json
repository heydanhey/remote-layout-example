{
  "name": "layout-next",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "bundles/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "heroku-prebuild": "cd apps/web && yarn && NODE_ENV=production yarn build",
    "start": "cd apps/web && yarn start"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": "16.14.2"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
